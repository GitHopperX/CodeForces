#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int N;
    cin >> N;
    long long n = N * 3;
    long long arr[n];
    for(int i = 0; i < n ; i++)
    {
        cin >> arr[i];
    }
    int sum = 0;
    for(int i = 0; i < n ; i++)
    {
        sum = sum + arr[i];
    }
    if(sum == 0)
    {
        cout << "YES" << endl;
    }
    else
    {
        cout << "NO" << endl;
    }
    return 0;
}
