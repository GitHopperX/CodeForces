#include <iostream>

using namespace std;

int main()
{
    string numberA, numberB, numberC = "";
    cin >> numberA >> numberB;
    int sz = numberA.size();

    for(int i = 0; i < sz; i++)
    {
        if(numberA[i] != numberB[i])
        {
            numberC = numberC + '1';
        }
        else
            numberC = numberC + '0';
    }
    cout << numberC << endl;
}
